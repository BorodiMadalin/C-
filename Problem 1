using System;

namespace ConsoleApp1
{
    class Program
    {
        
            static void Main(string[] args)
            {
                string inputData = Console.ReadLine(); // citim primul numÄƒr
                int a = Convert.ToInt32(inputData);
            if (a % 2 == 0) {

                Console.WriteLine("True");
            }
            else
            {
                Console.WriteLine("False");
            }
            }
               
            }
     
    }
